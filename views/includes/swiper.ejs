
<head>
    <!-- Link Swiper's CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
    <link rel="stylesheet" href="/styles/homeCarousel.css">
</head>
<div id="swapbox">
  <h1 id="heading2">Latest Products: </h1>
<section class="collection">
  <div class="swiper mySwiper">
    <div class="swiper-wrapper">
<%for(let i = allBrands[0].products.length - 1; i>= allBrands[0].products.length - 7; i--){%>
<div class="content swiper-slide">
 <a href= "/products/<%=allBrands[0].products[i]._id%>" ><img src="<%=allBrands[0].products[i].mainimage%>"></a>
</div>
<%}%>
  </div>
  </div>
</section>
</div>

  <!-- Swiper JS -->
  <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
  <script src="/javaScript/homeCarousel.js"></script>
</body>

</html>